{
  "name": "Skyy",
  "version": "1.0.2",
  "description": "",
  "manifest_version": 2,
  "icons": {
    "512": "LogoSky.png",
    "192": "LogoSky.png"
  },
  "browser_action": {
    "default_icon": "LogoSky.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "geolocation"
  ]
}