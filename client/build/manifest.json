{
  "name": "Lets goo!",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "512": "logo512.png",
    "192": "logo192.png"
  },
  "browser_action": {
    "default_icon": "logo512.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "geolocation"
  ]
}