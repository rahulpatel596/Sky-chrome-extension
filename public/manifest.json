{
  "name":"Lets goo!",
  "description":"Nice background with some useful information",
  "version":"1.0",
  "manifest_version": 2,
  "browser_action":{
    "default_popup":"index.html",
    "default_title":"Lets goo!"
  },
  "background":{
    "scripts":["background.js"]
  },
  "icons":{
    "16":"logo.svg"
  },
  "chrome_url_overrides":{
    "newTab":"index.html"
  },
  "permissions": ["storage","scripting"]
}